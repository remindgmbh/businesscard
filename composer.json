{
    "name": "remind/rmnd-businesscard",
    "description": "rmnd_businesscard.",
    "type": "library",
    "keywords": ["vcard", "vcf", "library"],
    "license": ["GPL-3.0-or-later"],
    "authors": [
        {
            "name": "REMIND GmbH",
            "email": "technik@remind.de"
        },
        {
            "name": "Hauke Schulz",
            "email": "h.schulz@remind.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Remind\\BusinessCard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Remind\\BusinessCard\\Tests\\": "Tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "vendor-dir": ".build/vendor",
        "bin-dir": ".build/bin"
    },
    "require": {
        "php": "^7.1.0 || ^7.2.0",
        "ramsey/uuid": "^3.4.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13.1",
        "pdepend/pdepend": "^2.5.2",
        "phploc/phploc": "^4.0.0",
        "phpmd/phpmd": "^2.6.0",
        "phpunit/phpunit": "^6.0.0",
        "squizlabs/php_codesniffer": "^3.3.2",
        "sebastian/phpcpd": "^3.0.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.remind.de/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true
}
