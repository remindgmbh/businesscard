{
    "name": "remind/rmnd-businesscard",
    "description": "rmnd_businesscard.",
    "type": "library",
    "keywords": ["vcard", "vcf", "library"],
    "license": ["GPL-3.0-or-later"],
    "authors": [
        {
            "name": "REMIND GmbH",
            "email": "technik@remind.de"
        },
        {
            "name": "Hauke Schulz",
            "email": "h.schulz@remind.de"
        }
    ],
    "require": {
        "ramsey/uuid": "^3.4.1"
    },
    "autoload": {
        "psr-4": {
            "Remind\\BusinessCard\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true
    },
    "require": {
        "php": "^7.1.0",
        "typo3/cms-core": "^8.7.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:remindgmbh/rmnd-businesscard.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true
