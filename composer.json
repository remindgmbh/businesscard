{
    "name": "remind/rmnd-businesscard",
    "description": "rmnd_businesscard.",
    "type": "library",
    "keywords": ["vcard", "vcf", "library"],
    "license": ["GPL-3.0-or-later"],
    "authors": [
        {
            "name": "REMIND GmbH",
            "email": "technik@remind.de"
        },
        {
            "name": "Hauke Schulz",
            "email": "h.schulz@remind.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Remind\\BusinessCard\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "vendor-dir": ".build/vendor",
        "bin-dir": ".build/bin"
    },
    "require": {
        "php": "^7.1.0",
        "ramsey/uuid": "^3.4.1",
        "typo3/cms-core": "^8.7.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.remind.de/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".build/web"
        }
    }
}
